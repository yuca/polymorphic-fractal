{
    "name": "yuca/polymorphic-fractal",
    "type": "library",
    "description": "Polymorphic Transformer implementation for PHP League's Fractal package.",
    "keywords": [
        "yuca",
        "polymorphic-fractal"
    ],
    "homepage": "https://github.com/yuca/polymorphic-fractal",
    "license": "MIT",
    "authors": [
        {
            "name": "Hrvoje Jukic",
            "email": "hrcajuka@gmail.com",
            "homepage": "https://github.com/yuca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "mouf/alias-container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yuca\\PolymorphicFractal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuca\\PolymorphicFractal\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
